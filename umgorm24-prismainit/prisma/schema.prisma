generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model usuarios {
id Int @id @default(autoincrement())
nombre String
apellido String
}

model sensor {
id Int @id @default(autoincrement())
value Decimal
device String
ip String
date DateTime @default(now())
}

model datos {
id Int @id @default(autoincrement())
nombre String
correo String
carne String
Latitud String
Longitud String
Humedad Float
Temperatura Float
}
